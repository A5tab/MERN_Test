#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules/chromedriver/bin/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules/chromedriver/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules/chromedriver/bin/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules/chromedriver/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/chromedriver@143.0.0/node_modules:/mnt/e/Dev/Devops/WebApp_Testing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/chromedriver@143.0.0/node_modules/chromedriver/bin/chromedriver" "$@"
else
  exec node  "$basedir/../.pnpm/chromedriver@143.0.0/node_modules/chromedriver/bin/chromedriver" "$@"
fi
